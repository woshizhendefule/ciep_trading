<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chengyu.ciep_trading.mapper.GoodsMapper">

    <resultMap id="BaseResultMap" type="com.chengyu.ciep_trading.domain.Goods">
